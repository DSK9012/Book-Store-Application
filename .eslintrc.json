{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "linebreak-style": 0,
    "comma-dangle": 0,
    "import/newline-after-import": 0,
    "consistent-return": 0,
    "object-curly-newline": 0
  }
}
